<h1>Edit Song Form</h1>
<%=form_for(@song) do |f| %>
<label>Name:</label>
<%=f.text_field :name %>
<br>
<label>Artist ID:</label>
<%=f.number_field :artist_id %>
<%=@song.artist.name%>
<br>
<label>Genre ID:</label>
<%=f.number_field :genre_id %>
<%=@song.genre.name%>
<br>
<%=f.submit 'Update Song' %>
<% end %>
